import java.util.Scanner;

public class fido {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = 1;
		int b = 1;
		int want = sc.nextInt();
		int cnt = 3;
		while (true) {
			int c= a+b;
			if(c==want) {
				System.out.print(want+"는 "+cnt+"번째 항입니다.");
				break;
			}
			a=b;
			b=c;
			cnt++;
		}
		
	}

}
