import java.util.Arrays;
import java.util.Iterator;
import java.util.Scanner;

public class W3Day5 {
	static Scanner sc = new Scanner(System.in);

	public static void main(String[] args) {
		int[] arr = {12, 57, 33, -1};
		System.out.print((arr==arr.clone())+" ");
		System.out.println(arr.length==arr.clone().length);
		
		for(int i = 0; i<arr.length;i++) {
			System.out.print((arr[i]==arr.clone()[i]));
		}
		System.out.println();
		System.out.println(Arrays.toString(arr).equals(Arrays.toString(arr.clone())));
	}

}
