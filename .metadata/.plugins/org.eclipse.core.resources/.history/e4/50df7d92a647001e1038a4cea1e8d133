import java.util.*;

class Solution {
	int maxx;
	int maxy;
    public  int[] solution(String[] park, String[] routes) {
    	
        int[] answer = {};
        maxx = park[0].length();
        maxy = park.length;
        int start[]= new int[2];
        for(int i = 0;i<park.length;i++) {
        	if(park[i].contains("S")) {
        	start[1] = park[i].indexOf("S");
        	start[0] = i;
        	}
        }
        for(int i = 0; i < routes.length;i++) {
        	routes[i].split(" ");
        }
        System.out.println(Arrays.toString(start));
        return answer;
    }
    public int mw(String[] park ,String move, int[] loca) {
    	int mloca = 0;
		/* for(int i = 0;i<) */
    	return mloca;
    }
}


