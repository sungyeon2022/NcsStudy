import java.util.Arrays;

public class mypractice {

	public static void main(String[] args) {
		int[] arr = {4,1,3,5,6,2,7,10};
		for(int i = 0; i<arr.length;i++) {
			for(int j = 0;j<i;j++) {
				if(arr[j]>arr[j+1]) {
					int t = arr[j];
					arr[j]=arr[j+1];
					arr[j+1]=t;
					System.out.println(Arrays.toString(arr));
				}
			}
		}
	}
	

	
}
