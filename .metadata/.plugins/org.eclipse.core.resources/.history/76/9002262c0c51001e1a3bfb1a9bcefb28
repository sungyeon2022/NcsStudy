
public class monte {

	public static void main(String[] args) {
		double cnt = 0;
		double isquadrant = 0;
		
		while (true) {
			double x = (int)(Math.random()*10000001);
			double y = (int)(Math.random()*10000001);
			if((x*x)+(y*y)<=1) {
				isquadrant++;
			}
			cnt++;
			
			System.out.println(x);
		}

	}

}
