
public class monte {

	public static void main(String[] args) {
		double cnt = 0;
		double isquadrant = 0;
		double x = Math.random()*10000000;
		double y = Math.random()*10000000;
		while (true) {
			if((x*x)+(y*y)<=1) {
				isquadrant++;
			}
			cnt++;
			System.out.println(cnt);
			System.out.println(isquadrant/cnt*4);
		}

	}

}
